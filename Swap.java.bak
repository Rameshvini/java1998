class Swap
{
public static void main(String []args)
{
int karthik=20;
int ramesh=30;
int temp=0;
System.out.println("Before swap:"+karthik);
System.out.println("Before swap:"+ramesh);
System.out.println("Before swap:"+temp);
temp=karthik;
karthik=ramesh;
ramesh=temp;
System.out.println("After swap:"+karthik);
System.out.println("After swap:"+ramesh);
System.out.println("After swap:"+temp);

}
}

