class Operator
{
	public static void main(String args[])
	{
		int pen=10;
		pen+=1;
		short pencil=15;
		pencil-=1;
		long eraser=5;
		eraser*=2;
		byte scale=6;
		scale/=2;
		double chair=20;
		chair%=5;
		System.out.println(pen);
		System.out.printl(eraser);
		System.out.printl(scale);
		System.out.printl(chair);
		System.out.printl(pencil);
		
	}
}

